public class Car {

    private String name;
    private String brand;
    private int plate;
    
    public Car() {
	name = "";
	brand = "";
	plate = 0;
    }

    public Car( String n, String b, int p ) {
	name = n;
	brand = b;
	plate = p;
    }

    public String getName() {
	return name;
    }
    
    public String getBrand() {
	return brand;
    }
    
    public int getPlate() {
	return plate;
    }

    public void setName( String s ) {
	name = s;
    }

    public void setBrand( String s ) {
	brand = s;
    }

    public void setPlate( int i ) {
	plate = i;
    }

    public String toString() {
	return ( "Car Name: " + name + "\nBrand: " + brand + "\nPlate#: " + plate; )
    }

}
